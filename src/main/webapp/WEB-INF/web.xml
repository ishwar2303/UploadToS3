<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>UploadToS3</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
      <servlet-name>UploadToS3</servlet-name>
      <servlet-class>com.upload.UploadToS3</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>UploadToS3</servlet-name>
      <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <servlet-name>AdminRegistration</servlet-name>
      <servlet-class>com.app.RegisterAdmin</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>AdminRegistration</servlet-name>
      <url-pattern>/admin-register</url-pattern>
  </servlet-mapping>

  <servlet>
      <servlet-name>FetchAdmins</servlet-name>
      <servlet-class>com.app.FetchAdmin</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>FetchAdmins</servlet-name>
      <url-pattern>/admins</url-pattern>
  </servlet-mapping>
  
  
  

</web-app>